    <insert id="insert" parameterType="java.util.List">

        insert all
        <foreach collection="list" item="i" separator=",">
            into
                file_info
                values
            (   f_seq.nextval,
                #{i.oriname},
                #{i.sysname},
                'Y',
                #{i.b_seq}
                )
        </foreach>
        SELECT * FROM DUAL
    </insert>

    <insert id="insertMap" parameterType="java.util.HashMap">
        insert
            into
            file_info
            values
            (
            f_seq.nextval,
            #{oriname},
            #{sysname},
            'Y',
            #{b_seq}
            )
    </insert>
</mapper>